@model Cumulative_1.Models.Course

<h1>Course Details</h1>

<table>
    <thead>
        <tr>
            <th>Course Name</th>
            <th>Course Code</th>
            <th>Teacher ID</th>
            <th>Start Date</th>
            <th>Finish Date</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.coursename</td>
            <td>@Model.coursecode</td>
            <td>@Model.teacherid</td>
            <td>@Model.startdate.ToString("yyyy-MM-dd")</td>
            <td>@Model.finishdate.ToString("yyyy-MM-dd")</td>
        </tr>
    </tbody>
</table>

<div class="mt-30">
    <a class="btn btn-danger" asp-area="" asp-controller="CoursePage" asp-action="List">Back</a>
</div>
